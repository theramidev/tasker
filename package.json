{
  "name": "tasker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npm run gradlewClean && react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && gradlew assembleRelease",
